{
    "expo": {
        "name": "Operation Mobile",
        "slug": "operation-mobile-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/logo1.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./src/assets/logo1.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/logo1.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.atss.operationmobile"
        },
        "web": {
            "favicon": "./src/assets/logo1.png"
        },
        "plugins": [
            [
                "expo-build-properties",
                {
                    "android": {
                        "usesCleartextTraffic": true
                    }
                }
            ]
        ]
    }
}